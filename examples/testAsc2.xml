<run id="mcmc" spec='beast.core.MCMC' chainLength="1000000" preBurnin="0">
        <state storeEvery='1000'>
          <parameter name='stateNode' id='coalescenceRate' value='10'/>

		  <!-- the following values may or may not be appropriate. Read the manual for further directions -->          
          <parameter name='stateNode' id='v' value='1.0' lower='0.0'/>
          <parameter name='stateNode' id='u' value='1.0' lower='0.0'/>
          <parameter name='stateNode' id='alpha'  value='2' lower='0.0'/>
          <parameter name='stateNode' id='beta'   value='200' lower='0.0'/>
          <parameter name='stateNode' id='lambda' value='10' lower='0.0'/>
		  <stateNode clusterType="upgma" estimate="true" id="tree" nodetype="snap.NodeData" spec="beast.util.ClusterTree" taxa='@alignment'/>
          <stateNode spec="beast.core.parameter.IntegerParameter" dimension="2" id='ascSiteCount' value='100 100' lower='0'/>
        </state>

        <distribution id="posterior" spec='CompoundDistribution'>
     	     <snapprior name='distribution' id='prior' alpha='@alpha' beta='@beta' lambda='@lambda' tree='@tree' coalescenceRate='@coalescenceRate'/>
 <!--
    	         <distr spec="beast.math.distributions.Exponential" mean="1000"/>
    	     <prior id="ascCountPrior" name="distribution" x="@ascSiteCount">
    	         <distr spec="beast.math.distributions.OneOnX"/>
             </prior>
 -->

             <snaptreelikelihood name='distribution' id='treeLikelihood' initFromTree='false' 
				pattern='theta' data='@alignment' tree='@tree' ascSiteCount='@ascSiteCount' 
				non-polymorphic="false">
                <siteModel spec='sitemodel.SiteModel' id="siteModel">
                    <substModel spec='snap.likelihood.SnapSubstitutionModel'
                        mutationRateU='@u' mutationRateV='@v'
            	        coalescenceRate='@coalescenceRate'/>
                </siteModel>
            </snaptreelikelihood>
        </distribution>

<!--
        <stateDistribution idref='prior'/>
-->

        <operator id="swapper" spec='operators.NodeSwapper' weight='0.5' tree='@tree'/>
        <operator id="budger" spec='operators.NodeBudger' weight='5' size='0.5' tree='@tree'/>
        <operator id="scaler" spec='operators.ScaleOperator' scaleFactor='0.25' weight='1' tree='@tree'/>
        <operator id="mover" spec='operators.GammaMover' scale='0.5' weight='0.5' coalescenceRate='@coalescenceRate'/>
        <operator id="mixer" spec='operators.RateMixer' scaleFactors='0.25' weight='1' tree='@tree'  coalescenceRate='@coalescenceRate'/>

<!-- estimate u and v -->
	<operator id="mutator" spec='operators.MutationMover' window='0.1' weight='0.25' u='@u' v='@v'/>

<!-- estimate ascertainment counts -->
        <operator id="c-swapper" spec='snap.operators.CompoundConstantSitesSampler' weight='0.1' ascSiteCount='@ascSiteCount' treelikelihood="@treeLikelihood" operator='@swapper'/>
        <operator id="c-budger"  spec='snap.operators.CompoundConstantSitesSampler' weight='0.1' ascSiteCount='@ascSiteCount' treelikelihood="@treeLikelihood" operator='@budger'/>
        <operator id="c-scaler"  spec='snap.operators.CompoundConstantSitesSampler' weight='0.1' ascSiteCount='@ascSiteCount' treelikelihood="@treeLikelihood" operator='@scaler'/>
        <operator id="c-mover"   spec='snap.operators.CompoundConstantSitesSampler' weight='0.1' ascSiteCount='@ascSiteCount' treelikelihood="@treeLikelihood" operator='@mover'/>
        <operator id="c-mixer"   spec='snap.operators.CompoundConstantSitesSampler' weight='0.1' ascSiteCount='@ascSiteCount' treelikelihood="@treeLikelihood" operator='@mixer'/>
        <operator id="c-mutator" spec='snap.operators.CompoundConstantSitesSampler' weight='0.1' ascSiteCount='@ascSiteCount' treelikelihood="@treeLikelihood" operator='@mutator'/>

        <logger logEvery="1000">
            <!--log idref="u"/>
            <log idref="v"/-->
            <log idref="posterior"/>
            <log idref="treeLikelihood"/>
            <log idref="prior"/>
            <!--log idref="normalised.Likelihood"/>
            <log idref="normalised.Posterior"/-->
    	    <log idref='ascSiteCount'/>
	        <!--log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/-->
        </logger>
        <logger logEvery="1000" fileName="testAsc2.log">
	        <model idref='posterior'/>
            <log idref="u"/>
            <log idref="v"/>
            <log idref="prior"/>
<!--
            <log id="normalised.Likelihood" spec='snap.likelihood.NormalisedDistributionLogger' treelikelihood='@treeLikelihood' distribution='@treeLikelihood'/>
            <log id="normalised.Posterior" spec='snap.likelihood.NormalisedDistributionLogger' treelikelihood='@treeLikelihood' distribution='@posterior'/>
-->
            <log idref="treeLikelihood"/>
            <log idref="posterior"/>
    	    <log idref='coalescenceRate'/>
    	    <log spec='snap.ThetaLogger' coalescenceRate='@coalescenceRate'/>
	        <log spec='beast.evolution.tree.TreeHeightLogger' tree='@tree'/>
    	    <log spec='TreeLengthLogger' tree='@tree'/>
    	    <log idref='ascSiteCount'/>
        </logger>
        <logger logEvery="1000" fileName="testAsc2.trees" mode='tree'>
            <log spec='beast.evolution.tree.TreeWithMetaDataLogger' tree="@tree" metadata='@coalescenceRate'/>
        </logger>
        
<!--
        <logger logEvery="1000" fileName="test2.$(seed).trees" mode='tree'>
            <log spec='snap.CoalescentUnitTreeLogger' tree="@tree" coalescenceRate='@coalescenceRate'/>
        </logger>
-->
</run>

