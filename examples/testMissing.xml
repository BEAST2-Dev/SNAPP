        <logger logEvery="1000" fileName="test2.$(seed).trees" mode='tree'>
            <log spec='snap.CoalescentUnitTreeLogger' tree="@tree" coalescenceRate='@coalescenceRate'/>
        </logger>
-->
</run>

</beast>

